{
    "name": "mrlaozhou/laravel-extend",
    "description": "Laravel extend.",
    "keywords": ["extend", "laravel", "collection", "mrlaozhou"],
    "require": {
        "php": ">= 7.0",
        "spatie/url": "^1.3"
    },
    "authors": [
        {
            "name": "Mrlaozhou",
            "email": "zgsself@163.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~7.3",
        "laravel/framework": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Mrlaozhou\\Extend\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrlaozhou\\Extend\\ExtendServiceProvider"
            ],
            "aliases": {
                "Expand": "Mrlaozhou\\Extend\\Facades\\Expand"
            }
        }
    }
}
